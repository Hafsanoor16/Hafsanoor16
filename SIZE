#include <iostream>

using namespace std;

int main()
{
    int size;
    cout<<"the size of array";
    cin>>size;

    int*arr=new int [size];
    cout<<"Enter element of the array";
    for(int i=0;i<size;i++){

        cin>>arr[i];

    }
    for (int i=0;i<size;i++){
        cout<<arr[i]<<" ";
    }
    delete[]arr;

 return 0;
}


