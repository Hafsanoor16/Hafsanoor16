#include <iostream>
#include <string>

using namespace std;
int main(){

  int age;

  cout<<"Eligible for driving license"<<endl;
  cin>>age;

  if(age >=18){

    cout<<"You are eligible for a driving license"<<endl;
  }

  else

    cout<<"sorry you are not eligible based on your age"<<endl;

 return 0;

  }


